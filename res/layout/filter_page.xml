<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:gravity="right">
<ScrollView android:id="@+id/filterScrollView01"
android:layout_width="fill_parent" android:background="@drawable/t7_480x720"
android:layout_height="fill_parent">
<LinearLayout
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:paddingLeft="5dip"
    android:paddingRight="5dip"
    android:paddingBottom="70dip"
    android:background="#00E0FFFF"
    android:focusable="true"
    android:focusableInTouchMode="true">

<LinearLayout
    android:layout_width="fill_parent" android:layout_height="wrap_content"
    android:background="@drawable/custom_button3"
    android:orientation="horizontal">
<LinearLayout
    android:layout_width="0dp" android:gravity="left"  android:layout_weight="1" 
    android:layout_height="wrap_content"
    android:layout_marginTop="7dip"
    android:layout_marginBottom="5dip" android:orientation="vertical">
<TextView android:id="@+id/filterHeadline1" 
    android:layout_height="wrap_content" 
    android:text="Full Name"
    android:textColor="#82CAFF"
    android:textSize="18dip"
    android:layout_width="fill_parent"/>
<TextView android:id="@+id/filterHeadline2" 
    android:layout_height="wrap_content" 
    android:text="My Practic, County"
    android:textColor="#FFFFFF"
    android:textSize="14dip"
    android:layout_width="fill_parent"/>
</LinearLayout>
    
</LinearLayout>

<LinearLayout
    android:layout_width="fill_parent" android:layout_height="wrap_content"    
    android:layout_marginTop="0dip" android:layout_marginBottom="0dip"
    android:orientation="horizontal" android:baselineAligned="false">
    
<LinearLayout
    android:layout_width="wrap_content"  android:layout_height="wrap_content"    
    android:layout_marginTop="0dip"  android:layout_marginBottom="0dip" 
    android:orientation="horizontal" android:baselineAligned="false">
<Button android:id="@+id/filter_offline_button" 
		android:layout_height="wrap_content" 
		android:text="Offline" android:textColor="#ffffff"	android:textSize="13dip" android:singleLine="true"
		android:background="@drawable/segment_button_left_on"
		android:layout_marginTop="5dp" android:layout_width="wrap_content">
</Button>
<Button android:id="@+id/filter_online_button" 
		android:layout_height="wrap_content" 
		android:text="Online" android:textColor="#808080"	android:textSize="13dip" android:singleLine="true"
		android:background="@drawable/segment_button_right"
		android:layout_marginTop="5dp" android:layout_width="wrap_content">
</Button>
</LinearLayout>

<LinearLayout
    android:layout_weight="1" android:layout_width="0dip"
    android:layout_height="wrap_content"  android:gravity="right"
    android:layout_marginTop="0dip" android:layout_marginBottom="0dip" 
    android:orientation="horizontal" android:baselineAligned="false"> 
<Button android:id="@+id/filter_doctor_button" 
		android:layout_height="wrap_content" 
		android:text="Doctor" android:textColor="#ffffff"	android:textSize="13dip" android:singleLine="true"
		android:background="@drawable/segment_button_left_on"
		android:layout_marginTop="5dp" android:layout_width="wrap_content">
</Button>
<Button android:id="@+id/filter_resource_button" 
		android:layout_height="wrap_content" 
		android:text="Other Resources" android:textColor="#808080"	android:textSize="13dip" android:singleLine="true"
		android:background="@drawable/segment_button_right"
		android:layout_marginTop="5dp" android:layout_width="wrap_content">
</Button>
	
<ToggleButton android:text="ToggleButton" android:id="@+id/online_search_toggle" 
	android:layout_marginRight="5dip"
	android:width="60dip" android:visibility="gone"
	android:textOn="Online" android:textOff="Offline"
    android:layout_width="wrap_content" android:layout_height="wrap_content"></ToggleButton>
    
<ToggleButton android:text="ToggleButton" android:id="@+id/filter_res_toggle"
    android:textOn="Resource" android:textOff="Doctor"  android:visibility="gone"
	android:width="75dip" android:layout_marginRight="5dip"
    android:layout_width="wrap_content" android:layout_height="wrap_content"></ToggleButton>	
</LinearLayout>
</LinearLayout>
<!-- 	NEW -->
<LinearLayout
    android:id="@+id/filterACOBtn" android:orientation="horizontal" android:visibility="visible"
    android:layout_width="fill_parent" android:layout_height="wrap_content"
    android:background="@drawable/custom_button3" android:clickable="true"
    android:paddingTop="10dip" android:paddingBottom="10dip"  android:gravity="right"
    android:layout_marginTop="5dip" android:layout_marginBottom="0dip">
    
<LinearLayout
    android:orientation="vertical"
    android:layout_width="wrap_content" android:layout_height="wrap_content" android:gravity="left"
    >
<TextView android:id="@+id/filterACO" 
    android:layout_width="wrap_content" android:layout_height="wrap_content" 
    android:text="ACO" android:textColor="#FFFFFF" android:textSize="20dip" android:singleLine="true" 
    android:layout_marginTop="0dip" android:layout_marginRight="5dip"
 />
</LinearLayout>
<TextView  android:id="@+id/filterACOValue"
    android:layout_weight="1" android:layout_width="0dip" 
    android:layout_height="wrap_content"  android:gravity="right"
    android:singleLine="true" 
    android:layout_marginTop="5dip"
    android:text="All"
    android:textColor="#FFFFFF99"
    android:textSize="15dip"/>
</LinearLayout>
<!-- 	NEW -->

<LinearLayout
    android:id="@+id/filterInsuranceBtn" android:orientation="horizontal"
    android:layout_width="fill_parent" android:layout_height="wrap_content"
    android:background="@drawable/custom_button3" android:clickable="true"
    android:paddingTop="10dip" android:paddingBottom="10dip"  android:gravity="right"
    android:layout_marginTop="5dip" android:layout_marginBottom="0dip">
    
<LinearLayout
    android:orientation="vertical"
    android:layout_width="wrap_content" android:layout_height="wrap_content" android:gravity="left"
    >
<TextView android:id="@+id/filterInsurance" 
    android:layout_width="wrap_content" android:layout_height="wrap_content" 
    android:text="Insurance" android:textColor="#FFFFFF" android:textSize="20dip" android:singleLine="true" 
    android:layout_marginTop="0dip" android:layout_marginRight="5dip"
    />
</LinearLayout>
<TextView  android:id="@+id/filterInsuranceValue"
    android:layout_weight="1" android:layout_width="0dip" 
    android:layout_height="wrap_content"  android:gravity="right"
    android:singleLine="true" 
    android:layout_marginTop="5dip"
    android:text="All"
    android:textColor="#FFFFFF99"
    android:textSize="15dip"/>
</LinearLayout>

<LinearLayout
    android:id="@+id/filterPracticeBtn" android:orientation="horizontal"
    android:layout_width="fill_parent" android:layout_height="wrap_content"
    android:background="@drawable/custom_button3" android:clickable="true" android:visibility="gone"
    android:paddingTop="10dip" android:paddingBottom="10dip"  android:gravity="right"
    android:layout_marginTop="5dip" android:layout_marginBottom="0dip">
    
<LinearLayout
    android:orientation="vertical"
    android:layout_width="wrap_content" android:layout_height="wrap_content" android:gravity="left"
    >
<TextView android:id="@+id/filterPractice" 
    android:layout_width="wrap_content" android:layout_height="wrap_content" 
    android:text="Practice" android:textColor="#FFFFFF" android:textSize="20dip"  
    android:layout_marginTop="0dip" android:layout_marginRight="5dip"
    />
</LinearLayout>
<TextView  android:id="@+id/filterPracticeValue"
    android:layout_weight="1" android:layout_width="0dip" 
    android:layout_height="wrap_content"  android:gravity="right"
    android:singleLine="true" 
    android:layout_marginTop="5dip"
    android:text="None"
    android:textColor="#FFFFFF99"
    android:textSize="15dip"/>
</LinearLayout>

<LinearLayout
    android:id="@+id/filterSpecialityBtn" android:orientation="horizontal"
    android:layout_width="fill_parent" android:layout_height="wrap_content"
    android:background="@drawable/custom_button3" android:clickable="true"
    android:paddingTop="10dip" android:paddingBottom="0dip"  android:gravity="right"
    android:layout_marginTop="5dip" android:layout_marginBottom="0dip">
    
<LinearLayout
    android:orientation="vertical"
    android:layout_width="wrap_content" android:layout_height="wrap_content" android:gravity="left"
    >
<TextView android:id="@+id/filterSpeciality" 
    android:layout_width="wrap_content" android:layout_height="wrap_content" 
    android:text="Speciality" android:textColor="#FFFFFF" android:textSize="20dip" android:singleLine="true" 
    android:layout_marginTop="0dip" android:layout_marginRight="5dip"
    />
    <CheckBox android:id="@+id/sees_in_petient"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"  android:layout_marginLeft="-5dip"
        android:text="Sees inpatients" />
</LinearLayout>
<TextView  android:id="@+id/filterSpecialityValue"
    android:layout_weight="1" android:layout_width="0dip" 
    android:layout_height="wrap_content"  android:gravity="right"
    android:singleLine="true" 
    android:layout_marginTop="5dip"
    android:text="All"
    android:textColor="#FFFFFF99"
    android:textSize="15dip"/>
</LinearLayout>

<LinearLayout
    android:id="@+id/filterHospitalBtn" android:orientation="horizontal"
    android:layout_width="fill_parent" android:layout_height="wrap_content"
    android:background="@drawable/custom_button3" android:clickable="true"
    android:paddingTop="10dip" android:paddingBottom="10dip"  android:gravity="right"
    android:layout_marginTop="5dip" android:layout_marginBottom="0dip">
    
<LinearLayout
    android:orientation="vertical"
    android:layout_width="wrap_content" android:layout_height="wrap_content" android:gravity="left"
    >
<TextView android:id="@+id/filterHospital" 
    android:layout_width="wrap_content" android:layout_height="wrap_content" 
    android:text="Hospital" android:textColor="#FFFFFF" android:textSize="20dip"  
    android:layout_marginTop="0dip" android:layout_marginRight="5dip"
    />
</LinearLayout>
<TextView  android:id="@+id/filterHospitalValue"
    android:layout_weight="1" android:layout_width="0dip" 
    android:layout_height="wrap_content"  android:gravity="right"
    android:singleLine="true" 
    android:layout_marginTop="5dip"
    android:text="All"
    android:textColor="#FFFFFF99"
    android:textSize="15dip"/>
</LinearLayout>

<LinearLayout
    android:id="@+id/filterCountyBtn" android:orientation="horizontal"
    android:layout_width="fill_parent" android:layout_height="wrap_content"
    android:background="@drawable/custom_button3" android:clickable="true"
    android:paddingTop="10dip" android:paddingBottom="10dip"  android:gravity="right"
    android:layout_marginTop="5dip" android:layout_marginBottom="0dip">
    
<LinearLayout
    android:orientation="vertical"
    android:layout_width="wrap_content" android:layout_height="wrap_content" android:gravity="left"
    >
<TextView android:id="@+id/filterCounty" 
    android:layout_width="wrap_content" android:layout_height="wrap_content" 
    android:text="County" android:textColor="#FFFFFF" android:textSize="20dip" android:singleLine="true" 
    android:layout_marginTop="0dip" android:layout_marginRight="5dip"
    />
