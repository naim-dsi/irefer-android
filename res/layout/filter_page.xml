<TextView  android:id="@+id/filterCountyValue"
    android:layout_weight="1" android:layout_width="0dip" 
    android:layout_height="wrap_content"  android:gravity="right"
    android:singleLine="true" 
    android:layout_marginTop="5dip"
    android:text="All"
    android:textColor="#FFFFFF99"
    android:textSize="15dip"/>
</LinearLayout>

<EditText android:id="@+id/zip_code_text_edit" android:visibility="gone"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dip"
                android:hint="ZIP Code"
                android:onClick="scrollUpView"
                android:inputType="number"
                android:singleLine="true" />

<EditText android:id="@+id/doc_name_text_edit"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dip"
                android:hint="Doctor Name"
                android:visibility="gone"
                android:singleLine="true" /> 

<LinearLayout
    android:id="@+id/filterLanguageBtn" android:orientation="horizontal" android:visibility="gone"
    android:layout_width="fill_parent" android:layout_height="wrap_content"
    android:background="@drawable/custom_button3" android:clickable="true"
    android:paddingTop="10dip" android:paddingBottom="10dip"  android:gravity="right"
    android:layout_marginTop="5dip" android:layout_marginBottom="0dip">
    
<LinearLayout
    android:orientation="vertical"
    android:layout_width="wrap_content" android:layout_height="wrap_content" android:gravity="left"
    >
<TextView android:id="@+id/filterLanguage" 
    android:layout_width="wrap_content" android:layout_height="wrap_content" 
    android:text="Language(s)" android:textColor="#FFFFFF" android:textSize="20dip"  
    android:layout_marginTop="0dip" android:layout_marginRight="5dip"
    />
</LinearLayout>
<TextView  android:id="@+id/filterLanguageValue"
    android:layout_weight="1" android:layout_width="0dip" 
    android:layout_height="wrap_content"  android:gravity="right"
    android:singleLine="true" 
    android:layout_marginTop="5dip"
    android:textColor="#FFFFFF99"
    android:textSize="15dip"/>
</LinearLayout>





<LinearLayout android:visibility="gone"
    android:id="@+id/filterOfficeHourBtn" android:orientation="horizontal"
    android:layout_width="fill_parent" android:layout_height="wrap_content"
    android:background="@drawable/custom_button3" android:clickable="true"
    android:paddingTop="10dip" android:paddingBottom="10dip"  android:gravity="right"
    android:layout_marginTop="5dip" android:layout_marginBottom="0dip">
    
<LinearLayout
    android:orientation="vertical"
    android:layout_width="wrap_content" android:layout_height="wrap_content" android:gravity="left"
    >
<TextView android:id="@+id/filterOfficeHour" 
    android:layout_width="wrap_content" android:layout_height="wrap_content" 
    android:text="Office Hours" android:textColor="#FFFFFF" android:textSize="20dip"  
    android:layout_marginTop="0dip" android:layout_marginRight="5dip"
    />
</LinearLayout>
<TextView  android:id="@+id/filterOfficeHourValue"
    android:layout_weight="1" android:layout_width="0dip" 
    android:layout_height="wrap_content"  android:gravity="right"
    android:singleLine="true" 
    android:layout_marginTop="5dip"
    android:text="All"
    android:textColor="#FFFFFF99"
    android:textSize="15dip"/>
</LinearLayout>

<Button android:id="@+id/filter_avvance_button" 
		android:layout_height="wrap_content" 
		android:text="Show Advance Search" android:textColor="#FFF8C6"	android:textSize="15dip" android:singleLine="true"
		android:background="@drawable/custom_action_bar_button"
		android:layout_marginTop="5dp" android:layout_width="wrap_content">
		</Button>
		
</LinearLayout>
</ScrollView>
<LinearLayout
       android:id="@+id/setup_bottom_wrapper"
       android:layout_height="wrap_content" android:orientation="horizontal"
       android:layout_width="fill_parent" android:background="#DD101010"  android:padding="0dp"
       android:layout_alignParentBottom="true" android:baselineAligned="true" android:gravity="center">
       
    	<RelativeLayout
		     android:layout_width="fill_parent"
		     android:layout_height="wrap_content"
		     android:background="@android:color/black"
		     android:gravity="bottom"
		     android:orientation="horizontal" >
		
		     <Button android:id="@+id/bottomInfoBtn" 
				android:layout_height="48dp" 
				android:layout_width="52dp"
				android:text="" 
				android:textColor="#FFF8C6"	
				android:singleLine="true"
				android:background="@drawable/action_bar_help"
				android:layout_marginLeft="2dp" 
				android:layout_marginRight="2dp" 
				android:layout_marginTop="4dp" 
				android:layout_marginBottom="4dp"  
				android:layout_alignParentLeft="true">
			</Button>
			<Button
		        android:id="@+id/filter_bottom_button1" 
				android:layout_height="48dp" 
				android:layout_width="52dp" 
				android:background="@drawable/action_bar_setup"
				android:onClick="setupClicked"
				android:paddingTop="0dp"
				android:layout_marginLeft="2dp" 
				android:layout_marginRight="2dp" 
				android:layout_marginTop="4dp" 
				android:layout_marginBottom="4dp"  
				
				android:layout_toRightOf="@+id/bottomInfoBtn"
				>
				
			</Button>
		   		
			<Button 
			    android:id="@+id/filter_bottom_button2" 
				android:layout_height="48dp" 
				android:layout_width="52dp"
				android:text="" 
				android:textColor="#33AA00"
				android:singleLine="true"
				android:background="@drawable/action_bar_text_search"
				android:layout_marginLeft="2dp" 
				android:layout_marginRight="2dp" 
				android:layout_marginTop="4dp" 
				android:layout_marginBottom="4dp"  
				android:layout_toLeftOf="@+id/getDoctorBtn">
			</Button>
			        
			<Button 
			    android:id="@+id/getDoctorBtn" 
				android:layout_height="48dp" 
				android:layout_width="52dp"
				android:text="" 
				android:textColor="#FFF8C6"	
				android:singleLine="true"
				android:background="@drawable/action_bar_search"
				android:layout_marginLeft="2dp"
				android:layout_marginRight="2dp"
				android:layout_marginTop="4dp"
				android:layout_marginBottom="4dp"
				android:layout_alignParentRight="true">
			</Button>
		</RelativeLayout>
    
    
    
    
    
    
        		 
    </LinearLayout> 
</RelativeLayout>
	